{
    "name": "litecms/contact",
    "description": "Contact package for litecms.",
  "keywords": [
    "lavalite",
    "Contact"
  ],
    "license": "MIT",
  "type": "lavalite-package",
  "homepage": "https://github.com/Litecms/Contact",
    "authors": [
        {
            "name": "Lavalite",
            "email": "info@lavalite.org"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Litecms\\Contact\\": "src",
            "Litecms\\Contact\\Seeders\\": "database/seeders"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Litecms\\Contact\\Providers\\ContactServiceProvider"
            ],
            "aliases": {
                "Contact" : "Litecms\\Contact\\Facades\\Contact"
            }
        }
    },
    "minimum-stability": "dev"
}
