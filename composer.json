{
  "name": "litecms/contact",
  "description": "Contact package for litecms.",
  "keywords": [
    "lavalite",
    "Contact"
  ],
  "license": "MIT",
  "type": "lavalite-package",
  "homepage": "https://github.com/Litecms/Contact",
  "authors": [
    {
      "name": "Lavalite",
      "email": "info@lavalite.org"
    }
  ],
  "require": {
    "php": "^8.0.2",
    "composer/installers": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "Litecms\\Contact\\": "src",
      "Litecms\\Contact\\Seeds\\": "database/seeds"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Litecms\\Contact\\ContactServiceProvider"
      ],
      "aliases": {
        "Contact": "Litecms\\Contact\\Facades\\Contact"
      }
    }
  },
  "minimum-stability": "dev"
}
